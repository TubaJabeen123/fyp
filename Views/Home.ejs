<style>
  
.home-team-section {
  position: relative;
  height: 100vh;
  width: 100%;
  overflow: hidden;
}

.slider-background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-size: cover;
  background-position: center;
  transition: opacity 1s ease;
}

.slider-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
}

.team-content {
  position: relative;
  z-index: 10;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
  padding: 0 20px;
}

.section-title {
  color: white;
  text-align: center;
  font-size: 2.5rem;
  font-weight: bold;
  margin-bottom: 3rem;
}

.role-cards {
  display: flex;
  justify-content: center;
  gap: 2rem;
  flex-wrap: wrap;
}

.role-card {
  width: 250px;
  background-color: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(8px);
  border-radius: 8px;
  overflow: hidden;
  transition: all 0.3s ease;
}

.role-card:hover {
  background-color: rgba(255, 255, 255, 0.2);
  box-shadow: 0 10px 25px rgba(0, 0, 0, 0.2);
}

.role-image {
  height: 250px;
  overflow: hidden;
}

.role-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  transition: transform 0.5s ease;
}

.role-card:hover .role-image img {
  transform: scale(1.1);
}

.role-title {
  padding: 1rem;
  text-align: center;
}

.role-title div {
  color: white;
  font-size: 1.25rem;
  font-weight: 600;
}

.slider-nav {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  z-index: 20;
  background-color: rgba(255, 255, 255, 0.2);
  backdrop-filter: blur(4px);
  color: white;
  width: 50px;
  height: 50px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  transition: background-color 0.3s ease;
}

.slider-nav:hover {
  background-color: rgba(255, 255, 255, 0.3);
}

.slider-prev {
  left: 20px;
}

.slider-next {
  right: 20px;
}

@media (max-width: 768px) {
  .role-cards {
    gap: 1rem;
  }

  .role-card {
    width: 200px;
  }

  .role-image {
    height: 200px;
  }
}

</style>

